<?xml version="1.0"?>
<Dotplotplugin
	id="org.dotplot.dpaas"
	name="DPaaS"
	version="1.0"
	provider="FH Giessen-Friedberg (www.fh-giessen.de)"
	info="Plugin for getting some Webservice-Mag1ck done">	

	<Dependency plugin="org.dotplot.core.Standard" version="1.0"/>
		<Service id="org.dotplot.addon.dpaas.webservice" 
			 class="org.dotplot.dpaas.WebService">
			 <Extention hotspot="org.dotplot.dpaas.webservice.binding"
			 	   class="java.lang.Object">
			 <Parameter name="endpoint" value="http://localhost:8080/dpaas" />
		</Extention>
		</Service>
	
	
</Dotplotplugin>